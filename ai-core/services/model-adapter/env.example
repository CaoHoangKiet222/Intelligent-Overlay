# Provider Keys (JSON format)
# Example: PROVIDER_KEYS='{"openai": "sk-...", "anthropic": "sk-ant-...", "ollama": "ollama"}'
PROVIDER_KEYS={"ollama":"ollama"}

# Database Configuration (if needed)
DB_DSN=postgresql+psycopg://ai:ai@localhost:5432/ai_core

# Redis Configuration (if needed)
REDIS_URL=redis://localhost:6379/0

# S3 Configuration (if needed)
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_GENERATION_MODEL=phi3:mini
OLLAMA_EMBEDDING_MODEL=qwen3-embedding:0.6b

# Task Routing (JSON format)
# Example: TASK_ROUTING='{"summary": "ollama", "qa": "ollama"}'
# TASK_ROUTING overrides TASK_ROUTING_DEFAULTS
TASK_ROUTING={}
# Default task routing (used if TASK_ROUTING doesn't specify a task)
TASK_ROUTING_DEFAULTS={"summary":"ollama","qa":"ollama","argument":"ollama","logic_bias":"ollama","sentiment":"ollama"}
